<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
         http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
         http://www.springframework.org/schema/context
         http://www.springframework.org/schema/context/spring-context-3.0.xsd">

    <context:property-placeholder location="classpath:session13/conf.properties"/>

    <bean id="man1" class="session13.Man">
        <constructor-arg value="3"/>
        <property name="name" ref="nameBean"/>
    </bean>

    <bean id="man2" class="session13.Man">
        <constructor-arg value="7"/>
        <constructor-arg value="${metalo}"/>
        <property name="age" value="12"/>
    </bean>

    <bean id="nameBean" class="java.lang.String">
        <constructor-arg value="Benedikt"/>
    </bean>
</beans>